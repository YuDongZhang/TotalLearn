<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:text="Fragment08"
        android:textSize="35sp"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/textView2" />

    <Button
        android:text="abs工厂"
        android:id="@+id/f8_b1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2" />

    <Button
        android:id="@+id/f8_b2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_toEndOf="@+id/f8_b1"
        android:text="原型模式1" />

    <Button
        android:id="@+id/f8_b3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/f8_b2"
        android:layout_toEndOf="@+id/f8_b2"
        android:text="适配器模式1" />

    <Button
        android:id="@+id/f8_b4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/f8_b3"
        android:layout_toEndOf="@+id/f8_b3"
        android:text="Button" />

    <Button
        android:id="@+id/f8_b5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f8_b1"
        android:layout_toStartOf="@+id/f8_b2"
        android:text="Button" />

    <Button
        android:id="@+id/f8_b6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f8_b2"
        android:layout_toEndOf="@+id/f8_b5"
        android:text="Button" />

    <Button
        android:id="@+id/f8_b7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f8_b3"
        android:layout_toStartOf="@+id/f8_b4"
        android:text="Button" />

    <Button
        android:id="@+id/f8_b9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/f8_b5"
        android:text="Button" />

    <Button
        android:id="@+id/f8_b10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/f8_b9"
        android:layout_toEndOf="@+id/f8_b5"
        android:text="桥接模式" />


</RelativeLayout>