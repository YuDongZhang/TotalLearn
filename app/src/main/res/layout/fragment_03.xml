<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:text="Fragment03"
        android:textSize="35sp"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/textView3" />

    <com.example.totallearn.customview.VerticalSeekBar
        android:id="@+id/vertical_Seekbar"
        android:layout_width="60dp"
        android:layout_height="450dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView3" />

    <Button
        android:id="@+id/f3_b1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_marginStart="34dp"
        android:layout_toEndOf="@+id/vertical_Seekbar"
        android:text="启动的测试Activity" />

    <Button
        android:id="@+id/f3_b2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/f3_b1"
        android:layout_below="@+id/f3_b1"
        android:text="Button" />

</RelativeLayout>